{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "abraham/twitteroauth": "^5.0",
        "christophrumpel/missing-livewire-assertions": "^1.0",
        "guzzlehttp/guzzle": "^7.5",
        "laravel/framework": "^10.1.0",
        "laravel/jetstream": "^3.0.1",
        "laravel/sanctum": "^3.2.1",
        "laravel/tinker": "^2.8.1",
        "livewire/livewire": "^2.12.1",
        "spatie/laravel-login-link": "^1.1.3",
        "spatie/laravel-webhook-client": "^3.1.7"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.13",
        "fakerphp/faker": "^1.21.0",
        "laravel/pint": "^1.6",
        "laravel/sail": "^1.21.0",
        "mockery/mockery": "^1.5.1",
        "nunomaduro/collision": "^6.4",
        "pestphp/pest": "^1.22.4",
        "pestphp/pest-plugin-laravel": "^1.4",
        "pestphp/pest-plugin-mock": "^1.0.3",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "ide-helper": [
            "@php artisan optimize:clear",
            "@composer dump-autoload",
            "@php artisan ide-helper:eloquent",
            "@php artisan ide-helper:eloquent",
            "@php artisan ide-helper:generate -M -H",
            "@php artisan ide-helper:models -N -r -p -M",
            "@php artisan ide-helper:meta"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
